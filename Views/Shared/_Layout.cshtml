<!DOCTYPE html>
<html lang="en">
<head>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1.0" />
	<title>@ViewData["Title"] - CruiseCMSDemo</title>
	<link rel="icon" href="img/favicon.png">
	<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />

	<!-- Custom CSS Libraries -->
	<link rel="stylesheet" href="~/lib/animate/css/animate.css">
	<link rel="stylesheet" href="~/lib/owl-carousel/css/owl-carousel.min.css">
	<link rel="stylesheet" href="~/lib/themify-icons/css/themify-icons.css">
	<link rel="stylesheet" href="~/lib/flaticon/css/flaticon.css">
	<link rel="stylesheet" href="~/lib/font-awesome/css/all.min.css">
	<link rel="stylesheet" href="~/lib/magnific-popup/css/magnific-popup.css">
	<link rel="stylesheet" href="~/lib/magnific-popup/css/gijgo.min.css">
	<link rel="stylesheet" href="~/lib/nice-select/css/nice-select.css">

	<link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
	<header>
		<nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
			<div class="container">
				<a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">CruiseCMSDemo</a>
				<button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
						aria-expanded="false" aria-label="Toggle navigation">
					<span class="navbar-toggler-icon"></span>
				</button>
				<div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
					<partial name="_LoginPartial" />
					<ul class="navbar-nav flex-grow-1">
						<li class="nav-item">
							<a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
						</li>
						<li class="nav-item">
							<a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
						</li>
					</ul>
				</div>
			</div>
		</nav>
	</header>
	<div class="container">
		<main role="main" class="pb-3">
			@RenderBody()
		</main>
	</div>

	<footer class="border-top footer text-muted">
		<div class="container">
			&copy; 2020 - CruiseCMSDemo - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
		</div>
	</footer>
	<script src="~/lib/jquery/dist/jquery.min.js"></script>
	<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>

	<!-- Custom JS Libraries -->
	<script src="~/lib/owl-carousel/js/owl-carousel.min.js"></script>
	<script src="~/lib/masonry/js/masonry.pkd.js"></script>
	<script src="~/lib/nice-select/js/nice-select.min.js"></script>
	<script src="~/lib/magnific-popup/js/jquery.magnific.popup.js"></script>
	<script src="~/lib/magnific-popup/js/gijgo.min.js"></script>

	<script src="~/js/site.js" asp-append-version="true"></script>
	@RenderSection("Scripts", required: false)
</body>
</html>
